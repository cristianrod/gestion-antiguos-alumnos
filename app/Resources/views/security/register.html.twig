{% extends 'base.html.twig' %}

{% block title %}Registarse{% endblock %}

{% block container %}
    {{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password.first) }}
    {{ form_row(form.password.second) }}

    <button type="submit">Registrate!</button>
    {{ form_end(form) }}
{% endblock %}